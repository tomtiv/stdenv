[core]
        repositoryformatversion = 0
        filemode = true
        bare = false
        logallrefupdates = true
        gitignore = .github/.gitignore_global

[remote "origin"]
        url = ssh://git@ssh.github.com:443/tomtiv/stdenv.git
        fetch = +refs/heads/*:refs/remotes/origin/*

[branch "origin"]
        remote = origin
        merge = refs/heads/origin

[remote "stdrem"]
         url = git@github.com:tomtiv/stdenv.git
         fetch = +refs/heads/*:refs/remotes/stdrem/*

[user]
	name = tomtiv
	email = tomtiv@gmail.com

[commit]
	template = .github/.stCommitMsg


